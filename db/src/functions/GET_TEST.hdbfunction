FUNCTION "GET_TEST"(iv_id NVARCHAR(255)) 
  RETURNS TABLE(
    "ID" NVARCHAR(255),
    "WAGETYPE" NVARCHAR(255),
    "UOM" NVARCHAR(5)
  ) 
  LANGUAGE SQLSCRIPT 
  SQL SECURITY INVOKER AS 
  BEGIN 
    RETURN
    SELECT "ID",
      "WAGETYPE",
      "UOM"
    FROM "COM_STRADA_VP_WAGETYPE";
END;
    